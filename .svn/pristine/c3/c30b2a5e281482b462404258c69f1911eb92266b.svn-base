/* FAQ */
.search-area{
    padding:15px 150px;
    margin-top:30px;
    border:1px solid #ddd;
    background-color:#fafafa;
}
.search-cont{
    position:relative;
    padding-left:185px;
}
.search-cont select{
    height:50px;
    min-width:180px;
    position:absolute;
    top:0;
    left:0;
}
.search-btn-box{
    display:inline-block;
    position:relative;
    border:1px solid #c9c9c9;
    box-sizing:border-box;
    border-radius:2px;
    background-color:#fff;
    padding-right:40px;
}
.search-btn-box input{
    width:100%;
    height:48px;
    border:0;
    background:none;
    outline:none;
}
.search-btn-box .search-btn{
    position:absolute;
    top:0;
    right:0;
}
.search-btn{
    display:inline-block;
    width:48px;
    height:48px;
    text-indent:-9999px;
    background:url('../images/common/search_icon_b.png') no-repeat 50%;
}
.faq-list{
    margin-top:20px;
}
.faq-box{
    border-top:2px solid #949494;
    border-bottom:1px solid #ddd;
}
.faq-box + dl{
    border-top:0;
}
.faq-box dt{
    position:relative;
    overflow:hidden;
    padding:20px 0 20px 75px;
    background:url('../images/common/ico_faq_down.png') no-repeat right 30px top 24px;
    cursor:pointer;
}
.faq-box .faq-tit{
    display:block;
    float:left;
    color:#222;
    font-size:15px;
    font-weight:bold;
}
.faq-box dt.active{
    background:url('../images/common/ico_faq_up.png') no-repeat right 30px top 29px;
    border-bottom:1px solid #ddd;
}
.faq-box dt.active .faq-tit{
    color:#d0102a;
}
.faq-box .faq-type{
    display:block;
    position:absolute;
    top:20px;
    left:10px;
    width:60px;
    color:#d0102a;
    font-size:15px;
    font-weight:bold;
    text-align:center;
}
.faq-box dd{
    display:none;
    position:relative;
    overflow:hidden;
    padding:20px 0 20px 75px;
    background-color:#fafafa;
}
.faq-box .faq-type.st2{
    color:#5a5a5a;
    line-height:30px;
}
.faq-box dd p{
    color:#5a5a5a;
    font-size:14px;
    line-height:30px;
}  

.info-box.right {
    padding-right: 0px; 
}

