/* 페이징 */
.paging-area{
    margin-top:30px;
    text-align:center;
}
.paging-area > a{
    display:inline-block;
    color:#666;
    font-size:16px;
    margin:0 8px;
    line-height:30px;
}
.paging-area > a.paging{
    width:30px;
    height:30px;
    border:1px solid #d0d0d0;
    box-sizing:border-box;
    text-indent:-9999px;
    margin:0;
}
.paging-area > a.paging.first{
    background:url("../images/common/ico_paging_first.png") no-repeat 50%;
}
.paging-area >  a.paging.next{
    background:url("../images/common/ico_paging_next.png") no-repeat 50%;
    margin-right:15px;
}
.paging-area > a.paging.last{
    background:url("../images/common/ico_paging_last.png") no-repeat 50%;
}
.paging-area > a.paging.prev{
    background:url("../images/common/ico_paging_prev.png") no-repeat 50%;
    margin-left:15px;
}
.paging-btn{
	margin-top:10px;
    float:right;
}
/* 효과 */
.paging-area > a:hover{
    color:#ce0015;
}
.paging-area > a.paging:hover{
    border:1px solid #ce0015;
}
.paging-area > a.active{
    color:#ce0015;
}

.view-paging{
    margin-top:10px;
    border-top:1px solid #2f2f2f;
}
.view-paging li{
    padding:15px 30px;
    border-bottom:1px solid #c9c9c9;
}
.view-paging li span{
    display:inline-block;
    color:#666;
    font-size:14px;
}
.view-paging li .paging{
    width:10%;
    padding-left:20px;
    box-sizing:border-box;
}
.view-paging li .paging.prev{
    background:url("../images/common/ico_view_paging_prev.png") no-repeat left 50%;
}
.view-paging li .paging.next{
    background:url("../images/common/ico_view_paging_next.png") no-repeat left 50%;
}
.view-paging li strong{
    display:inline-block;
    width:80%;
    color:#292929;
    font-size:14px;
    font-weight:normal;
}
.view-paging li strong a{
    display:inline-block;
    max-width:95%;
    color:#292929;
    font-size:14px;
    font-weight:bold;
    overflow:hidden;
    text-overflow:ellipsis;
    white-space:nowrap;
    vertical-align:-4px;
}
.view-paging li strong a:hover{
    color:#d0112b;
    text-decoration:underline;
}


