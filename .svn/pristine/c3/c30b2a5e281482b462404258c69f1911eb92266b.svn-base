.table-style01{
    width:100%;
    border-top:1px solid #2f2f2f;
    table-layout:fixed;
}
.table-style01 thead th{
    padding:15px 5px;
    color:#292929;
    font-size:14px;
    border-bottom:1px solid #c9c9c9;
    background-color:#fdf6f6;
}
.table-style01 tbody td{
    padding:14px 5px;
    color:#666;
    font-size:14px;
    text-align:center;
    border-bottom:1px solid #c9c9c9;
    display: table-cell;
    vertical-align: inherit;
}
.table-style01 tbody td.subject{
    text-align:left;
}
.table-style01 tbody td.subject a{
    display:inline-block;
    overflow:hidden;
    max-width:95%;
    text-overflow:ellipsis;
    white-space:nowrap;
    color:#292929;
    font-size:14px;
    vertical-align:middle;
}
.table-style01 tbody td.subject a:hover{
    text-decoration:underline;
    color:#ce0015;
}
.table-style01 tbody td.subject .file{
    display:inline-block;
    width:10px;
    height:15px;
    background:url("../images/common/ico_vos_link.png") no-repeat right 50%;
    text-indent:-9999px;
    vertical-align:middle;
}
/* 버튼 */
.b-btn-area{
    margin-top:20px;
    text-align:center;
}
.bbtn{
    display:inline-block;
    position:relative;
    overflow:hidden;
    padding:10px 36px 10px 36px;
    border:1px solid #d0d0d0;
    color:#292929;
    font-size:14px;
    transition:all .5s;
    z-index:99;
}
.bbtn.type2{
    background-color:#d0112b;
    color:#fff;
    border:1px solid #d0112b;
}
.bbtn:before{
    content:"";
    position:absolute; top:0; right:0; left:0; bottom:0;
    background-color:#d0112b;
    transform:translatex(-110%);
    z-index:-1;
    transition:all .5s;
}
.bbtn.type2:before{
    background-color:#fff;
}
.bbtn.type3{
    color:#d0112b;
    border-color:#d0112b;
}
.bbtn.s{
    padding:5px 18px 5px 18px;
}
.bbtn.s.ico1{
    padding-left:30px;
}
.bbtn.s.ico1:after{
    content:"";
    position:absolute; top:7px; left:13px;
    width:15px;
    height:15px;
    background:url("../images/common/ico_file_add.png") no-repeat 50%;
    transition:all .5s;
}
.bbtn.s.ico2{
    padding-left:30px;
}
.bbtn.s.ico2:after{
    content:"";
    position:absolute; top:7px; left:14px;
    width:15px;
    height:15px;
    background:url("../images/common/ico_file_del.png") no-repeat 50%;
    transition:all .5s;
}
/* 효과 */
.bbtn:hover{
    color:#fff;
    border:1px solid #d0112b;
}
.bbtn:hover:before{
    transform:translatex(0);
}
.bbtn.type2:hover{
    color:#d0112b;
    border:1px solid #d0112b;
}
.bbtn.type2:hover:before{
    transform:translatex(0);
}
.bbtn.s.ico1:hover:after{
    background:url("../images/common/ico_file_add_hover.png") no-repeat 50%;
}
.bbtn.s.ico2:hover:after{
    background:url("../images/common/ico_file_del_hover.png") no-repeat 50%;
}

