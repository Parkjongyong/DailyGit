	        <thead>
	            <tr>
	                <th>순번</th><th>파일명</th><th>용량</th>
	            </tr>
	        </thead>
	        <tbody>
	            <#list ATTACH_FILES as item>
	                <tr>
	                    <td style="text-align: center;">
	                        ${item.FILE_NO}
	                    </td>
	                    <td>
	                        ${item.FILE_NAME}
	                    </td>
	                    <td style="text-align: right;">
	                        ${item.FILE_SIZE_TEXT}
	                    </td>
	                </tr>
	            </#list>
	        </tbody>
	    </table>
	
	</page>
	</#if>
